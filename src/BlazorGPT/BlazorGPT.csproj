    <PackageReference Include="NRedisStack" Version="0.11.0" />
    <PackageReference Include="Radzen.Blazor" Version="4.23.4" />
    <PackageReference Include="SharpToken" Version="1.2.14" />

  </ItemGroup>

  <ItemGroup>
    <Content Update="wwwroot\css\chatgpt-conversations.css">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Update="wwwroot\css\material-dark-base.css">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Update="wwwroot\css\material-dark-custom.css">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Migrations\" />
  </ItemGroup>


  <ItemGroup>
    <Content Remove="wwwroot\state\**" />
    <Content Include="wwwroot\state\**" Watch="false" />
  </ItemGroup>


